# use umfpack_make.m to compile the MATLAB interface to UMFPACK

default:
	- echo "Use umfpack_make.m in MATLAB to compile the UMFPACK mexFunction"

# Remove all but the files in the original distribution
distclean:
	- $(RM) -r rename.h *.o *.mex*

purge: distclean

clean: distclean

